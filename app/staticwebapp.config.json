{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/images/*.{png,jpg,gif,ico,svg}",
      "/css/*",
      "/js/*",
      "/api/*",
      "/*.{json,xml,txt,md}"
    ]
  },
  "routes": [
    {
      "route": "/*",
      "serve": "/index.html",
      "statusCode": 200
    }
  ]
}